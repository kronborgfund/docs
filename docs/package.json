{
  "name": "@koksmat/docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules ",
    "docusaurus": "docusaurus",
    "gendoc": "docusaurus gen-api-docs all",
    "development": "export debug= && docusaurus start -p 3002",
    "build": "export debug=* && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve -p 3001",
    "inst": "pnpm install",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^1.1.0",
    "@docusaurus/core": "2.4.0",
    "@docusaurus/preset-classic": "2.4.0",
    "@docusaurus/theme-mermaid": "^2.4.0",
    "@mdx-js/react": "^1.6.22",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.16",
    "clsx": "^1.2.1",
    "color": "^4.2.3",
    "docusaurus-plugin-openapi-docs": "2.0.0-beta.2",
    "docusaurus-theme-openapi-docs": "2.0.0-beta.2",
    "postcss": "^8.4.31",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.4.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "docusaurus-plugin-typedoc": "^0.19.0",
    "typedoc": "^0.24.1",
    "typedoc-plugin-markdown": "^3.15.1",
    "typedoc-plugin-zod": "^1.0.1",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
